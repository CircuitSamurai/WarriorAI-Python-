import matplotlib.pyplot as plt
import numpy as np # numpy arrays are faster than python list
 
 # grid() - helps make plots easier to read by adding reference lines


x = [1, 2, 3, 4, 5]
y =[5, 10, 15, 20, 25]

plt.grid(axis="y",
         linewidth=2,
         color="orange",
         linestyle="dashed")

plt.plot(x, y)
plt.show()