import numpy as np


rng = np.random.default_rng()

print(rng.integers(1, 7, size=(3, 2)))

print(np.random.uniform(-1, 1, size=(3, 2)))

array = np.array([1, 2, 3, 4, 5])
rng.shuffle(array)
print(array)

fruits = np.array(["ğŸ", "ğŸŒ", "ğŸ’", "ğŸŠ", "ğŸ‡"])
fruits = rng.choice(fruits, size=(3, 2))
print(fruits)