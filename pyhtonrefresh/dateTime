import datetime

date = datetime.date(2025, 12, 25)
print(f"Date: {date}")

today = datetime.date.today()
print(f"Today: {today}")

time = datetime.time(14, 30, 45)
print(f"Time: {time}")

now = datetime.datetime.now()
now = now.strftime("%H:%M:%S %m-%d-%Y")
print(f"Current Time and Time: {now}")
